<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kdev.consultacep">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ConsultaCEP">
        <activity android:name=".ui.OracaoPaoActivity"></activity>
        <activity android:name=".NiceActivity" />
        <activity
            android:name=".navigation.EnderecoNavigation"
            android:label="@string/title_activity_endereco_navigation"
            android:theme="@style/Theme.ConsultaCEP.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>